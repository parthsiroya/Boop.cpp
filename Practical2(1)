#include<iostream>
using namespace std;
 int main()
 {
    double a;
    double b;
    double add;
    double sub;
    double mult;
    double div;

    cout << "enter a value of a and b";
    cin >> a >> b;
    add = a+b;
    sub = a-b;
    mult = a*b;
    div = a/b;

        cout <<"addition is" << add << endl;
        cout <<"subtrantion is" << sub << endl;
        cout <<"mutiplied is" << mult << endl;
        cout <<"divison is" << div << endl;
 }
