#include <iostream>
using namespace std;

inline int multiply(int num1, int num2) {
    return num1 * num2;
}

int main() {
    int num1, num2;
    cout << "Enter first number: ";
    cin >> num1;
    cout << "Enter second number: ";
    cin >> num2;

    int result = multiply(num1, num2);
    cout << "The result of multiplication is: " << result << endl;

    return 0;
}
